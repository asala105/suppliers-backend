    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/chai": "^5.0.1",
    "@types/express": "^5.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "chai": "^5.1.2",
    "mocha": "^11.0.1",
